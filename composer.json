{
    "name": "owowagency/laravel-user-removal",
    "description": "Reusable package for deleting users, which is obligated by the GDPR.",
    "keywords": [
        "laravel",
        "users",
        "removal"
    ],
    "authors": [
        {
            "name": "Dees Oomens",
            "email": "dees@owow.io"
        }
    ],
    "require": {
        "illuminate/support": "^10.0",
        "dees040/laravel-api-responses": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^8.8"
    },
    "autoload": {
       "psr-4": {
            "OwowAgency\\UserRemoval\\": "src/UserRemoval"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OwowAgency\\UserRemoval\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwowAgency\\UserRemoval\\UserRemovalServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
